{
  "name": "@insightflow/sdk",
  "version": "1.0.0",
  "description": "InsightFlow 用户行为分析 SDK - TypeScript版本",
  "type": "module",
  "main": "dist/insightflow-sdk.js",
  "module": "dist/insightflow-sdk.esm.js",
  "types": "dist/insightflow-sdk.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "rollup -c rollup.config.js",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "keywords": [
    "analytics",
    "tracking",
    "user-behavior",
    "typescript",
    "sdk"
  ],
  "author": "InsightFlow Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/user-activity-dashboard.git",
    "directory": "frontend/sdk"
  },
  "bugs": {
    "url": "https://github.com/your-org/user-activity-dashboard/issues"
  },
  "homepage": "https://github.com/your-org/user-activity-dashboard#readme"
} 