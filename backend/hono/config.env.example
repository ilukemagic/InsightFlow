# CORS配置（多个域名用逗号分隔）
ALLOWED_ORIGINS=*

# 日志配置
LOG_LEVEL=info

# 应用信息
APP_TITLE=InsightFlow BFF API
APP_DESCRIPTION=用户行为分析平台 - 多端数据聚合服务
APP_VERSION=1.0.0 