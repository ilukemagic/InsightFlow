# 服务配置
HOST=0.0.0.0
PORT=8000
NODE_ENV=development

# 外部服务
GOLANG_SERVICE_URL=http://localhost:8080
REDIS_URL=redis://localhost:6379

# 缓存配置（秒）
CACHE_TTL=60
DASHBOARD_CACHE_TTL=30
FUNNEL_CACHE_TTL=300

# HTTP客户端配置
