# 服务配置
HOST=0.0.0.0
PORT=8000
DEBUG=false
RELOAD=true

# 外部服务
GOLANG_SERVICE_URL=http://localhost:8080
REDIS_URL=redis://localhost:6379

# 缓存配置（秒）
CACHE_TTL=60
DASHBOARD_CACHE_TTL=30
FUNNEL_CACHE_TTL=300

# HTTP客户端配置
REQUEST_TIMEOUT=5.0
MAX_CONNECTIONS=100
MAX_KEEPALIVE_CONNECTIONS=20

# CORS配置（多个域名用逗号分隔）
ALLOWED_ORIGINS=*

# 日志配置
LOG_LEVEL=info 